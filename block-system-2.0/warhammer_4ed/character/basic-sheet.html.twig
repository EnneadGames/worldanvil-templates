                        </tr>
                        {% endfor %}
                    </table>
                    <table class='table'>
                        {% for i in 1..20 %}
                            {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}
                            {% set eo = 'od' %}{% if id is even %}{% set eo = 'ev' %}{% endif %}
                        <tr>
                            <th class='lbl od lbl-talent-name'>  </th><td class='var od var-talent-name'> {{attribute(variables, 'talent_name_' ~ id)|default}} </td>
                            <th class='lbl ev lbl-talent-taken'>  </th><td class='var ev var-talent-taken'> {{attribute(variables, 'talent_taken_' ~ id)|default}} </td>
                            <th class='lbl od lbl-talent-desc'>  </th><td class='var od var-talent-desc'> {{attribute(variables, 'talent_desc_' ~ id)|default}} </td>
                        </tr>
                        {% endfor %}
                    </table>
                </div>
            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="card ambitions" id="card-ambitions">
                <div class="card-header ambitions">Ambitions</div>
                <div class="card-body">
                    <div class='cBox'><div class='lbl od lbl-amb-short'> Short Term </div><div class='var od var-amb-short'> {{variables.amb_short|default|nl2br}} </div></div>
                    <div class='cBox'><div class='lbl ev lbl-amb-long'> Long Term </div><div class='var ev var-amb-long'> {{variables.amb_long|default|nl2br}} </div></div>
                </div>
            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="card party" id="card-party">
                <div class="card-header party">Party</div>
                <div class="card-body">
                    <div class='cBox'><div class='lbl od lbl-party-name'> Party Name </div><div class='var od var-party-name'> {{variables.party_name|default}} </div></div>
