                            <th class='lbl ev lbl-gaskill-skill-label'> Skill </th><td class='var ev var-gaskill-skill-label'></td>
                        </tr>
                        {% endfor %}
                    </table>
                    <table class='table'>
                        {% for i in 1..20 %}
                            {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}
                            {% set eo = 'od' %}{% if id is even %}{% set eo = 'ev' %}{% endif %}
