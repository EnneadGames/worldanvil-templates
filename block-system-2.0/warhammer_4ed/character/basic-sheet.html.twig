                            <th class='lbl od lbl-gaskill-adv'>  </th><td class='var od var-gaskill-adv'> {{attribute(variables, 'gaskill_adv_' ~ id)|default}} </td>
                            <th class='lbl ev lbl-gaskill-skill'>  </th><td class='var ev var-gaskill-skill'> {{attribute(variables, 'gaskill_skill_' ~ id)|default}} </td>
                        </tr>
                        {% endfor %}
                    </table>
                </div>
            </div>
        </div>
        <div class="col-12 col-md-6">
            <div class="card talents" id="card-talents">
                <div class="card-header talents">Talents</div>
                <div class="card-body">
                    <table class='table'>
                        {% for i in 1..1 %}
                            {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}
                            {% set eo = 'od' %}{% if id is even %}{% set eo = 'ev' %}{% endif %}
