{% macro doSign(x = 0) %}{% if x != 0 %}{% if x > 0 %}+{% endif %}{{x}}{% endif %}{% endmacro doSign %}
{% macro doSkill(var, id, sk, scb_comb, scb_comm, scb_mani, scb_ment, scb_perc, scb_phys, use_scb, use_sscb) %}{% set name = attribute(var, 'skill_name_' ~ id)|default %}{% set lbl = 'var' %}
{% if name == "" %}{% set name = sk[1]|default %}{% endif %}{% set tag = "td" %}{% if sk[3]|default == "c" %}{% set tag = "th" %}{% set lbl = 'lbl' %}{% endif %}
{% set v = attribute(var, 'skill_value_' ~ id)|default %}{% set pl = sk[2]|default %}{% set tt = '' %}
{% if name == "Gaming" %}{% set pl = (var.int|default + var.pow|default + scb_ment) ~ " %" %}{% endif %}
{% if name == "Fly" %}{% set pl = (var.dex|default * 4 + scb_phys) ~ " %"  %}{% endif %}
{% if name == "Projection" %}{% set pl = (var.dex|default * 2 + scb_phys) ~ " %" %}{% endif %}
{% if name == "Dodge" %}{% set pl = (var.dex|default * 2 + scb_comb) ~ " %" %}{% endif %}
<tr>{% set eo = "od" %}{% if id is even %}{% set eo = "ev" %}{% endif %}
  <{{tag}} class='i{{lbl}} {{eo}} i{{lbl}}-skill-name' title='Skill Name'>
    {% set skg = attribute(var, 'skill_group_' ~ id)|default %}
    {% if skg == "" %}{% set skg = sk[5] %}{% endif %}
    {% set scb = 0 %}
    {% if name == "Combat" or skg == "Combat" %}
      {% set scb = scb_comb %}
      {% if use_scb > 0 %}{% set tt = 'DEX,INT,STR' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'DEX' %}{% endif %}
    {% endif %}
    {% if name == "Communication" or skg == "Communication" %}
      {% set scb = scb_comm %}
      {% if use_scb > 0 %}{% set tt = 'INT,POW,CHA' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'CHA' %}{% endif %}
    {% endif %}
    {% if name == "Manipulation" or skg == "Manipulation" %}
      {% set scb = scb_mani %}
      {% if use_scb > 0 %}{% set tt = 'DEX,INT,STR' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'DEX' %}{% endif %}
    {% endif %}
    {% if name == "Mental" or skg == "Mental" %}
      {% set scb = scb_ment %}
      {% if use_scb > 0 %}{% set tt = 'INT,POW' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'INT' %}{% endif %}
    {% endif %}
    {% if name == "Perception" or skg == "Perception" %}
      {% set scb = scb_perc %}
      {% if use_scb > 0 %}{% set tt = 'INT,POW,CON' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'POW' %}{% endif %}
    {% endif %}
    {% if name == "Physical" or skg == "Physical" %}
      {% set scb = scb_phys %}
      {% if use_scb > 0 %}{% set tt = 'DEX,STR,CON,-SIZ' %}{% endif %}
      {% if use_sscb > 0 %}{% set tt = 'STR' %}{% endif %}
    {% endif %}
    {% if name == skg %}
    <input value='{{name}}' id='skill_name' name='skill_name_{{id}}' type='hidden' />{{name}} {% if tt != '' %}<span class='float-right tt'>( {{tt}} )</span>{% endif %}
    {% else %}
      <input value='{{name}}' class='form-control ivar ivar-skill-name' id='skill_name' name='skill_name_{{id}}' type='text' />
      {% set scb2 = scb %}{% set scb = 0 %}
    {% endif %}
  </{{tag}}><{{tag}} class='i{{lbl}} {{eo}} ivar-skill-value c' title='Skill Value'>
    <input title='Default: {{pl + scb2}} + value' value='{{v}}' class='form-control ivar ivar-skill-value c' id='skill_value' name='skill_value_{{id}}' type='number' placeholder='{{scb}}' />
  </{{tag}}><{{tag}} class='i{{lbl}} {{eo}} ivar-skill-bonus c' title='Skill Bonus'>
    {{ sheet.doSign(pl + scb2) }}
  </{{tag}}><{{tag}} class='i{{lbl}} {{eo}} ivar-skill-group'>
    {% if name == skg %}
    <input value='{{skg}}' id='skill_group' name='skill_group_{{id}}' type='hidden' />&nbsp;
    {% else %}
    <select class='form-control ivar ivar-skill-group' id='skill_group' name='skill_group_{{id}}'>
      <option value='Communication' {% if skg == 'Communication' %}selected='selected' {% endif %} > Communication </option>
      <option value='Manipulation' {% if skg == 'Manipulation' %}selected='selected' {% endif %} > Manipulation </option>
      <option value='Mental' {% if skg == 'Mental' %}selected='selected' {% endif %} > Mental </option>
      <option value='Perception' {% if skg == 'Perception' %}selected='selected' {% endif %} > Perception </option>
      <option value='Physical' {% if skg == 'Physical' %}selected='selected' {% endif %} > Physical </option>
      <option value='Combat' {% if skg == 'Combat' %}selected='selected' {% endif %} > Combat </option>
    </select>
    {% endif %}
  </{{tag}}>  
  {% if name == skg %}
  <{{tag}} class='i{{lbl}} {{eo}} ivar-skill-mark c'>
    &nbsp;
  </{{tag}}>
  {% else %}
  <{{tag}} class='i{{lbl}} {{eo}} ivar-skill-mark c' title='Check(ed) for advancement roll'>
    <input value='0' id='skill_mark' name='skill_mark_{{id}}' type='hidden' /><input value='1' class='c' {% if attribute(var, 'skill_mark_' ~ id)|default > 0 %} checked='checked'{% endif %} id='skill_mark' name='skill_mark_{{id}}' type='checkbox' />
  </{{tag}}>
  {% endif %}
</tr>{% endmacro doSkill %}
{# -------------------------------------------------------------- #}
{% set style = 'light' %}{% if variables.style|default == 1 %}{% set style = 'dark' %}{% endif %}
<div class="container-fluid sheet-alana-roleplaying-{{style}}">
  <div class="row">
    <div class="col-12">
    <center><h1>Alana Roleplaying</h1></center>
    </div>
    <div class="col-12">
      <div class="card personal" id="card-personal">
        <div class="card-header personal"> Personal
          <span class='float-right'><input value='0' id='style' name='style' type='hidden' /><input value='1' class='c' {% if variables.style|default > 0 %} checked='checked'{% endif %} id='style' name='style' type='checkbox' /> Dark Mode</span>
        </div>
        <div class="card-body">
          <div class="row">
            <div class="col-12 col-md-7">
              <table class='table'>
                <tr><th class='ilbl od ilbl-name' width='40%'><label for='name'>Name</label></th><td class='ivar od ivar-name'><input value='{{variables.name|default}}' class='form-control ivar ivar-name' id='name' name='name' placeholder='Name of the hero' type='text' required=required /></td></tr>
                <tr><th class='ilbl ev ilbl-gender'><label for='gender'>Gender</label></th><td class='ivar ev ivar-gender'><input value='{{variables.gender|default}}' class='form-control ivar ivar-gender' id='gender' name='gender' type='text' placeholder='put gender or pronouns here'/></td></tr>
                <tr><th class='ilbl od ilbl-ancestry'><label for='ancestry'>Ancestry</label></th><td class='ivar od ivar-ancestry'><input value='{{variables.ancestry|default}}' class='form-control ivar ivar-ancestry' id='ancestry' name='ancestry' type='text' placeholder='eg Human'/></td></tr>
                <tr><th class='ilbl od ilbl-archetype1'><label for='archetype1'>Archetype (primary)</label></th><td class='ivar od ivar-archetype1'><input value='{{variables.archetype1|default}}' class='form-control ivar ivar-archetype1' id='archetype1' name='archetype1' type='text' placeholder='eg Noble, Soldier' /></td></tr>
                <tr><th class='ilbl od ilbl-archetype2'><label for='archetype2'>Archetype</label></th><td class='ivar od ivar-archetype2'><input value='{{variables.archetype2|default}}' class='form-control ivar ivar-archetype2' id='archetype2' name='archetype2' type='text' placeholder='eg Noble, Soldier' /></td></tr>
                <tr><th class='ilbl ev ilbl-wealth'><label for='wealth'>Wealth Level</label></th><td class='ivar ev ivar-wealth' >
                  <select  class='form-control ivar ivar-wealth' id='wealth' name='wealth'>
                    <option value='Destitute' {% if variables.wealth|default == 'Destitute' %}selected='selected' {% endif %} > Destitute </option>
                    <option value='Poor' {% if variables.wealth|default == 'Poor' %}selected='selected' {% endif %} > Poor </option>
                    <option value='Average' {% if variables.wealth|default == 'Average' %}selected='selected' {% endif %} > Average </option>
                    <option value='Affluent' {% if variables.wealth|default == 'Affluent' %}selected='selected' {% endif %} > Affluent </option>
                    <option value='Wealthy' {% if variables.wealth|default == 'Wealthy' %}selected='selected' {% endif %} > Wealthy </option>
                  </select></td></tr>
                <tr><th class='ilbl ev ilbl-culture'><label for='culture'>Culture</label></th><td class='ivar ev ivar-culture'><input value='{{variables.culture|default}}' class='form-control ivar ivar-culture' id='culture' name='culture' type='text' /></td></tr>
                <tr><th class='ilbl od ilbl-gods-religion'><label for='gods_religion'>Gods / Religion</label></th><td class='ivar od ivar-gods-religion'><input value='{{variables.gods_religion|default}}' class='form-control ivar ivar-gods-religion' id='gods_religion' name='gods_religion' type='text' /></td></tr>
              </table>
            </div>
            <div class="col-12 col-md-5">
              <table class='table'>
                <tr><th class='ilbl ev ilbl-reputation'><label for='reputation'>Reputation</label></th><td class='ivar ev ivar-reputation'><input value='{{variables.reputation|default}}' class='form-control ivar ivar-reputation' id='reputation' name='reputation' type='number' placeholder='0'/></td></tr>
                <tr><th class='ilbl od ilbl-move'><label for='move'>Movement (MOV)</label></th><td class='ivar od ivar-move'><input value='{{variables.move|default}}' class='form-control ivar ivar-move' id='move' name='move' type='number' placeholder='10'/></td></tr>
                <tr><th class='ilbl ev ilbl-handedness'><label for='handedness'>Handedness</label></th><td class='ivar ev ivar-handedness ' >
                  <select class='form-control ivar ivar-handedness' id='handedness' name='handedness'>
                    <option value='right' {% if variables.handedness|default == 'right' %}selected='selected' {% endif %} > right </option>
                    <option value='left' {% if variables.handedness|default == 'left' %}selected='selected' {% endif %} > left </option>
                    <option value='ambidextrous' {% if variables.handedness|default == 'ambidextrous' %}selected='selected' {% endif %} > ambidextrous </option>
                  </select></td></tr>
                <tr><th class='ilbl od ilbl-age'><label for='age'>Age</label></th><td class='ivar od ivar-age'><input value='{{variables.age|default}}' class='form-control ivar ivar-age' id='age' name='age' type='number' placeholder='17+1d6'/></td></tr>
                <tr><th class='ilbl ev ilbl-height'><label for='height'>Height</label></th><td class='ivar ev ivar-height'><input value='{{variables.height|default}}' class='form-control ivar ivar-height' id='height' name='height' type='text' /></td></tr>
                <tr><th class='ilbl od ilbl-weight'><label for='weight'>Weight</label></th><td class='ivar od ivar-weight'><input value='{{variables.weight|default}}' class='form-control ivar ivar-weight' id='weight' name='weight' type='text' /></td></tr>
                <tr><th class='ilbl ev ilbl-portrait'><label for='portrait'>Portrait</label></th><td class='ivar ev ivar-portrait'><input value='{{variables.portrait|default}}' class='form-control ivar ivar-portrait' id='portrait' name='portrait' placeholder='WA image ID' type='number' /></td></tr>
                <tr><th class='ilbl od ilbl-player'><label for='player'>Player</label></th><td class='ivar od ivar-player'><input value='{{variables.player|default}}' class='form-control ivar ivar-player' id='player' name='player' placeholder='Your name' type='text' /></td></tr>
