                      <option value='Enquirer'      {% if variables.flair|default == 'Enquirer'      %}selected='selected' {% endif %}> Enquirer      </option>
                      <option value='Monk'          {% if variables.flair|default == 'Monk'          %}selected='selected' {% endif %}> Monk          </option>
                      <option value='Mythic'        {% if variables.flair|default == 'Mythic'        %}selected='selected' {% endif %}> Mythic        </option>
                      <option value='Noble'         {% if variables.flair|default == 'Noble'         %}selected='selected' {% endif %}> Noble         </option>
