  <div class="row">
    <div class="col-12">
      <div class="card skills" id="card-skills">
        <div class="card-header skills">Skills</div>
        <div class="card-body of">
          <span class="note">Note: set a skill to -1 to hide it from the sheet.</span>
          {% set use_scb = variables.use_scb|default %}
          {% set use_sscb = variables.use_sscb|default %}
          {% if use_scb == '' and use_sscb == '' %}{% set use_scb = 0 %}{% set use_sscb = 1%}{% endif %}
          <div class='ilbl ev ilbl-use-scb'><input value='0' id='use_scb' name='use_scb' type='hidden' /><input value='1' class='c' {% if use_scb > 0 %} checked='checked'{% endif %} id='use_scb' name='use_scb' type='checkbox' /><label for='use_scb'>&nbsp;&nbsp;Use Skill Category Bonuses</label></div>
          <div class='ilbl od ilbl-use-sscb'><input value='0' id='use_sscb' name='use_sscb' type='hidden' /><input value='1' class='c' {% if use_sscb|default > 0 %} checked='checked'{% endif %} id='use_sscb' name='use_sscb' type='checkbox' /><label for='use_sscb'>&nbsp;&nbsp;Use Simple Skill Category Bonuses</label></div>
          <span class="note">Note: 250 points for Primary Archetype, 100 points for Secondary Archetype, 100 points anywhere else.</span>
          {% set scb_comb = 0 %}
          {% set scb_comm = 0 %}
          {% set scb_mani = 0 %}
          {% set scb_ment = 0 %}
          {% set scb_perc = 0 %}
          {% set scb_phys = 0 %}
          {% if variables.use_sscb|default > 0 %}
            {% set scb_comb = (variables.dex|default + 1) // 2 %}
            {% set scb_comm = (variables.int|default + 1) // 2 %}
            {% set scb_mani = (variables.dex|default + 1) // 2 %}
            {% set scb_ment = (variables.int|default + 1) // 2 %}
            {% set scb_perc = (variables.int|default + 1) // 2 %}
            {% set scb_phys = (variables.dex|default + 1) // 2 %}
          {% endif %}
          {% if variables.use_scb|default > 0 %}
            {% set scb_comb = 2 * (variables.dex|default - 10) + ((variables.int|default - 10) // 2) + ((variables.str|default - 10) // 2) %}
            {% set scb_comm = 2 * (variables.int|default - 10) + ((variables.pow|default - 10) // 2) + ((variables.cha|default - 10) // 2) %}
            {% set scb_mani = 2 * (variables.dex|default - 10) + ((variables.int|default - 10) // 2) + ((variables.str|default - 10) // 2) %}
            {% set scb_ment = 2 * (variables.int|default - 10) + ((variables.pow|default - 10) // 2) %}
            {% set scb_perc = 2 * (variables.int|default - 10) + ((variables.pow|default - 10) // 2) + ((variables.con|default - 10) // 2) %}
            {% set scb_phys = 2 * (variables.dex|default - 10) + ((variables.str|default - 10) // 2) + ((variables.con|default - 10) // 2) - (variables.siz|default - 10) %}
          {% endif %}
          <div class="row">
            <div class="col-12 col-md-6">
              <table class='table'>
                <tr>
                  <th class='ilbl ev ilbl-skill-name'><label for='skill_name'>Name</label></th>
                  <th class='ilbl ev ilbl-skill-value c' width='60px'><label for='skill_value'>%</label></th>
                  <th class='ilbl ev ilbl-skill-bonus c' width='30px'><label for='skill_bonus'>+</label></th>
                  <th class='ilbl ev ilbl-skill-group' width="100px"><label for='skill_group'>Group</label></th>
                  <th class='ilbl ev ilbl-skill-mark c' width='30px'><label for='skill_mark'>C</label></th>
                </tr>
                {% for i in 1..49 %}
                  {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}{% if id < 100 %}{% set id = '0' ~ id %}{% endif %}
                  {% set sk = skill_list[id-1] %}
                  {{ sheet.doSkill(variables, id, sk, scb_comb, scb_comm, scb_mani, scb_ment, scb_perc, scb_phys, use_scb, use_sscb) }}
                {% endfor %}
              </table>
            </div>
            <div class="col-12 col-md-6">
              <table class='table'>
                <tr>
                  <th class='ilbl ev ilbl-skill-name'><label for='skill_name'>Name</label></th>
                  <th class='ilbl ev ilbl-skill-value c' width='60px'><label for='skill_value'>%</label></th>
                  <th class='ilbl ev ilbl-skill-bonus c' width='30px'><label for='skill_bonus'>+</label></th>
                  <th class='ilbl ev ilbl-skill-group' width="100px"><label for='skill_group'>Group</label></th>
                  <th class='ilbl ev ilbl-skill-mark c' width='30px'><label for='skill_mark'>C</label></th>
                  </tr>
                {% for i in 50..98 %}
                  {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}{% if id < 100 %}{% set id = '0' ~ id %}{% endif %}
                  {% set sk = skill_list[id-1] %}
                  {{ sheet.doSkill(variables, id, sk, scb_comb, scb_comm, scb_mani, scb_ment, scb_perc, scb_phys, use_scb, use_sscb) }}
                {% endfor %}
                </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
        <span class="note">Note: the following lists will show more empty lines when the visible ones fill up.</span>
    </div>
    <div class="col-12">
      <div class="card weapons" id="card-weapons">
        <div class="card-header weapons">Weapons</div>
        <div class="card-body of">
          <table class='table'>
            {% set clistmax = 30 %}{% set emptymin = 1 %}{% set listmin = 2 %}{% set ecount = 0 %}{% set count = 0 %}
            {% for i in 1..clistmax %}
                {% set id = i %}{% if id < 10 %}{% set id = '0' ~ id %}{% endif %}
                {% set name = attribute(variables, 'weapon_name_' ~ id)|default|trim %}
                {% if name == "" and i > listmin %}{% set ecount = ecount + 1 %}{% endif %}
                {% if ecount <= emptymin or i < listmin or name != "" %}
                  {% set count = count + 1 %}
                  {% set eo = 'od' %}{% if count is even %}{% set eo = 'ev' %}{% endif %}
                  {% if count == 1 or count // 11 == count / 11 %}
                  <tr>
                    <th class='ilbl od ilbl-weapon-type'><label for='weapon_type'>Type</label></th>
                    <th class='ilbl od ilbl-weapon-base c' width='60px' title='Base attack value of the weapon'><label for='weapon_base'>Base</label></th>
                    <th class='ilbl od ilbl-weapon-value c' width='60px'><label for='weapon_value'>%</label></th>
                    <th class='ilbl od ilbl-weapon-damage' width='27%'><label for='weapon_damage'>Damage, Special</label></th>
                    <th class='ilbl od ilbl-weapon-range c' width='80px'><label for='weapon_range'>Range</label></th>
                    <th class='ilbl od ilbl-weapon-attacks' width='60px'><label for='weapon_attacks'>Attacks</label></th>
                    <th class='ilbl od ilbl-weapon-length c' width='80px'><label for='weapon_length'>Length</label></th>
                    <th class='ilbl od ilbl-weapon-hand c' width='40px' title='1H or 2H'><label for='weapon_hand'>Hand</label></th>
                    <th class='ilbl od ilbl-weapon-hp c' width='60px'><label for='weapon_hp'>HP</label></th>
                    <th class='ilbl od ilbl-weapon-mark c' width='40px'><label for='weapon_mark'>C</label></th>
                  </tr>
                {% endif %}
                <tr>
                  <td class='ivar {{eo}} ivar-weapon-type' title='Weapon Name'><input value='{{attribute(variables, 'weapon_type_' ~ id)|default}}' class='form-control ivar ivar-weapon-type' id='weapon_type' name='weapon_type_{{id}}' type='text' /></td>
                  <td class='ivar {{eo}} ivar-weapon-base' title='Base attack value of the weapon'><input value='{{attribute(variables, 'weapon_base_' ~ id)|default}}' class='form-control ivar ivar-weapon-base c' id='weapon_base' name='weapon_base_{{id}}' type='number' /></td>
                  <td class='ivar {{eo}} ivar-weapon-value' title='Weapon Skill Value'><input value='{{attribute(variables, 'weapon_value_' ~ id)|default}}' class='form-control ivar ivar-weapon-value c' id='weapon_value' name='weapon_value_{{id}}' type='number' /></td>
                  <td class='ivar {{eo}} ivar-weapon-damage'><input value='{{attribute(variables, 'weapon_damage_' ~ id)|default}}' class='form-control ivar ivar-weapon-damage' id='weapon_damage' name='weapon_damage_{{id}}' type='text' /></td>
                  <td class='ivar {{eo}} ivar-weapon-range'><input value='{{attribute(variables, 'weapon_range_' ~ id)|default}}' class='form-control ivar ivar-weapon-range c' id='weapon_range' name='weapon_range_{{id}}' type='text' /></td>
                  <td class='ivar {{eo}} ivar-weapon-attacks'><input value='{{attribute(variables, 'weapon_attacks_' ~ id)|default}}' class='form-control ivar ivar-weapon-attacks c' id='weapon_attacks' name='weapon_attacks_{{id}}' type='number' /></td>
