{# ALL MACROS #}
{# MACROS GEN_SKILL #}
{% macro genSkill(id, name ='', min ='', value ='', dname ='', dmin ='', other ='') %}
    {% if name == '' %} {# if name is empty, get the default name (and possibly value also) #}
        {% if dname != '' %}{% set name = dname %}{% endif %}
    {% endif %}
    {% if min == '' %}{% set min = dmin %}{% endif %}
    {% if value != '' %}
        {% if min > value %}{% set value = min %}{% endif %}
    {% endif %}
    {% set eo = "od" %}{% if id is even %}{% set eo = "ev" %}{% endif %}
    <tr>
        <td class="{{eo}}"><input value="{{name}}" class="form-control" id="skill_name_{{id}}" name="skill_name_{{id}}" type="hidden" /> {{name}}</td>
        <td class="{{eo}}"><input value="{{min}}" class="form-control" id="skill_min_{{id}}" name="skill_min_{{id}}" type="hidden" /> ({{min}})</td>
        <td class="{{eo}}"><input value="{{value}}" class="form-control al" id="skill_value_{{id}}" name="skill_value_{{id}}" type="string" min="0" max="10" /></td>
    </tr>
{% endmacro genSkill %}
