{# ALL MACROS #}
{# MACROS GEN_SKILL #}
{% macro genSkill(id, name ='', min ='', value ='', dname ='', dmin ='', other ='') %}
    {% if name == '' %} {# if name is empty, get the default name (and possibly value also) #}
        {% if dname != '' %}{% set name = dname %}{% endif %}
    {% endif %}
    {% if min == '' %}{% set min = dmin %}{% endif %}
    {% if value != '' %}
        {% if min > value %}{% set value = min %}{% endif %}
    {% else %}
        {% set value = '-' %}
    {% endif %}
    {% if name != '' %}
        {% set eo = "od" %}{% if id is even %}{% set eo = "ev" %}{% endif %}
        <tr>
            <td class="skill-name {{eo}}">{{name}}</td>
            <td class="attribute-value {{eo}} w50">( {{min}} )</td>
            <td class="number-value {{eo}}">{{value}}</td>
        </tr>
    {% endif %}
{% endmacro genSkill %}
{# MACROS GEN_ACTIONS #}
{% macro genAction(id, name, goal, impact) %}
    {% if name is defined and name != '' %}
        <tr>
            {% set eo = "od" %}
            {% if id is even %}{% set eo = "ev" %}{% endif %}
            <td class="attribute-name {{eo}}">{{name}}</td>
            <td class="attribute-value {{eo}}">{{goal}}</td>
            <td class="attribute-value {{eo}}">{{impact}}</td>
        </tr>
    {% endif %}
{% endmacro genAction %}
{# MACROS PERKS #}
