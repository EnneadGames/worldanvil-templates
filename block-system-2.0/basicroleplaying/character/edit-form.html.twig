{% macro doSkill(var, id, sk) %}{% set name = attribute(variables, 'skill_name_' ~ id)|default %}
{% if name == "" %}{% set name = sk[1]|default %}{% endif %}{% set tag = "td" %}{% if sk[3]|default == "c" %}{% set tag = "th" %}{% endif %}
{% set v = attribute(var, 'skill_value_' ~ id)|default %}{% set pl = sk[2]|default %}
<tr>
  <{{tag}} class='var var-skill-name' title='Skill Name'>
    <input value='{{name}}' class='form-control ivar ivar-skill-name' id='skill_name' name='skill_name_{{id}}' type='text' />
  </{{tag}}><td width='180px' class='var {{eo}} var-skill-value c' title='Skill Value'>
    <input value='{{v}}' class='form-control ivar ivar-skill-value c' id='skill_value' name='skill_value_{{id}}' type='number' placeholder='{{pl}}' />
  </td><td class='var {{eo}} var-skill-mark c' title='checked'>
    <input value='0' id='skill_mark' name='skill_mark_{{id}}' type='hidden' /><input value='1' class='c' {% if attribute(var, 'skill_mark_' ~ id)|default > 0 %} checked='checked'{% endif %} id='skill_mark' name='skill_mark_{{id}}' type='checkbox' />
  </td>
</tr>{% endmacro doSkill %}
