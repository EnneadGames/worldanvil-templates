{% if dmb >= 25 and dmb <= 32 %}{% set dm = '+1d4' %}{% endif %}
{% if dmb >= 33 and dmb <= 40 %}{% set dm = '+1d6' %}{% endif %}
{% if dmb >= 41 and dmb <= 56 %}{% set dm = '+2d6' %}{% endif %}
{% if dmb >= 57 and dmb <= 72 %}{% set dm = '+3d6' %}{% endif %}
{% if dmb > 73 %}{% set dm = '+' ~ (3+(dmb-72)//16) ~ 'd6' %}{% endif %}
