{% macro doSkill(var, id, sk) %}{% set name = attribute(var, 'skill_name_' ~ id)|default %}
{% if name != "" %}
{% if name == "" %}{% set name = sk[1]|default %}{% endif %}
{% if not (name ends with ":") and name != '-' %}
{% set tag = "td" %}{% set class = "var var-" %}
{% if sk[3]|default == "c" %}{% set tag = "th" %}{% set class = "lbl od lbl-" %}{% endif %}
{% set v = attribute(var, 'skill_value_' ~ id)|default %}
{% if v != -1 %}
{% set grey = "" %}{% if v == "" %}
  {% if sk[0]|default != "" and sk[0]|default != "-" and sk[2]|default != "-" %}{% set grey = "grey" %}{% set v = sk[2]|default %}
  {% if v matches '/^\\d+$/' %}{% set v = v ~ " %" %}{% else %}
    {% set v = '' %}
    {% if name == "Gaming" %}{% set v = (var.int|default + var.pow|default) ~ " %" %}{% endif %}
    {% if name == "Fly" %}{% set v = (var.dex|default * 4) ~ " %"  %}{% endif %}
    {% if name == "Projection" %}{% set v = (var.dex|default * 2) ~ " %" %}{% endif %}
    {% if name == "Dodge" %}{% set v = (var.dex|default * 2) ~ " %" %}{% endif %}
  {% endif %}
  {% endif %}
{% else %}{% if v > 0 %}{% set v = v ~ " %" %}{% endif %}{% endif %}
<tr>
  <{{tag}} class='{{class}}skill-name {{grey}}' title='Skill Name'> {{name}} </{{tag}}>
  <{{tag}} class='{{class}}skill-value {{grey}} c' title='Skill Value'>{{v}}</{{tag}}>
  <{{tag}} class='{{class}}skill-mark c' title='Check(ed) for advancement roll'>{% if attribute(var, 'skill_mark_' ~ id)|default|default == 1 %}<i class='fa-regular fa-square-check'></i>{% else %}<i class='fa-regular fa-square'></i>{% endif %}</{{tag}}>
</tr>{% endif %}{% endif %}{% endif %}{% endmacro doSkill %}
{# -------------------------------------------------------------- #}
{% macro MarkMax(value = 0) %}{% if value > 50 %}<span class='marked'>{% endif %}{{value}}{% if value > 50 %}</span>{% endif %}{% endmacro MarkMax %}
